<?xml version="1.0" encoding="UTF-8"?> 
<launch> 
    <param name="/use_sim_time" value="true"/>

    <arg name="vocabulary" default="/root/catkin_ws/src/ORB_SLAM3/Vocabulary/ORBvoc.txt" doc="Vocabulary for DBoW2."/> 
    <arg name="settings" default="/root/catkin_ws/src/ORB_SLAM3/Examples/Stereo-Inertial/rosario_dataset/Rosario_3.yaml" doc="System settings."/> 
    <arg name="map_frame_id" default="/map"/>
    <arg name="base_link_frame_id" default="/base_link"/>
    <node name="orbslam3_stereo_inertial" pkg="ORB_SLAM3" type="Stereo_Inertial" output="screen" args="$(arg vocabulary) $(arg settings) true"> 
        <remap from="/camera/left/image_raw" to="/stereo/left/image_raw"/>
        <remap from="/camera/right/image_raw" to="/stereo/right/image_raw"/>
        <param name="map_frame_id" value="$(arg map_frame_id)"/>
        <param name="base_link_frame_id" value="$(arg base_link_frame_id)"/>
    </node>

    <node pkg="tf2_ros" type="static_transform_publisher" name="start_broadcaster" args="0 0 0 0 0 0 start map"/>
</launch>